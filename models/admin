const Moderator = require('./moderator');

//admin class inherits from moderator
class Admin extends Moderator {
	constructor(data){
	super(data);
		
		this.canDeleteComment = this.canDeleteComment.bind(this);
		
	}
	canEditAnyComment(){
		return true;
	}
	canDeleteAnyComment(){
		return true;
	}
}
export default Admin;